extend ../base
block stylesheets
    link(rel='stylesheet', href='/css/autocomplete.css')
    link(rel='stylesheet', href='/css/profile-wizard.css')
    link(rel='stylesheet', href='/vendor/angular-growl/build/angular-growl.min.css')
    
block scripts
    script(src='/vendor/jquery/dist/jquery.min.js')
    script(src='/vendor/angular/angular.js')
    script(src='/vendor/angular-ui-form-validation/dist/angular-ui-form-validation.js')
    script(src='//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.12.0.js')
    script(src='/vendor/ng-flow/dist/ng-flow-standalone.js')
    script(src='//cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.min.js')
    script(src='http://maps.googleapis.com/maps/api/js?libraries=places&sensor=false')
    script(src='/vendor/angular-cookies/angular-cookies.min.js')
    script(src='http://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.3/angular-sanitize.min.js')
    script(src='/vendor/angular-growl/build/angular-growl.js')
    script(src='/js/profileWizard.js')
    script(src='/js/ngAutocomplete.js')

block breadcrumbs
    .container
        ul.pull-left.breadcrumb
            li
                a(href='#{ url("home") }') Home
            li.active #{title}
block header
    include ../partials/navbar
block footer
    include ../partials/footer_nav
    include ../partials/copyright
block content
    .container
        // views will be injected here
        div(ui-view)
